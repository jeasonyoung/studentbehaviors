<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<!--菜单服务-->
	<bean id="MenuService" class="ipower.studentworks.service.impl.MenuServiceImpl"> 
		<property name="menuFile" value="main_menus.xml"/>
	</bean>
	<!--学校单位服务-->
	<bean id="UnitService" class="ipower.studentworks.service.impl.UnitServiceImpl">
		<property name="unitDao" ref="UnitDao"/>
	</bean>
	
</beans>